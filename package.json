{
  "name": "@valkonis/shmynamo",
  "version": "4.0.2",
  "description": "An implementation of Amazon's DynamoDB built on LevelDB",
  "homepage": "https://github.com/redwheelbarrow/shmynamo",
  "repository": {
    "type": "git",
    "url": "https://github.com/redwheelbarrow/shmynamo"
  },
  "types": "./src/index.d.ts",
  "bugs": "https://github.com/redwheelbarrow/shmynamo/issues",
  "exports": "./src/index.js",
  "main": "src/index.js",
  "scripts": {
    "t": "mocha --require should --timeout 10000 src/test/listTables.js",
    "build": "bash -c 'for file in ./src/db/*.pegjs; do pegjs \"$file\"; done'",
    "test": "mocha --require should --reporter spec -t $([ $REMOTE ] && echo 30s || echo 4s) src/test",
    "coverage": "yarn nyc mocha --require should -t 4s src/test",
    "lint": "eslint src --fix"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "yarn@4.9.4",
  "contributors": [
    {
      "name": "Michael Hart",
      "email": "michael.hart.au@gmail.com"
    },
    {
      "name": "redwheelbarrow",
      "email": "redwheelbarrow@valkonis.com"
    }
  ],
  "author": "Michael Hart <michael.hart.au@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/node": "^24.7.0",
    "async": "^3.2.4",
    "big.js": "^6.2.1",
    "buffer-crc32": "^0.2.13",
    "lazy": "^1.0.11",
    "level": "^10.0.0",
    "lock": "^1.1.0",
    "memory-level": "^3.0.0",
    "minimist": "^1.2.8",
    "once": "^1.4.0"
  },
  "devDependencies": {
    "@architect/eslint-config": "^3.0.0",
    "aws4": "^1.12.0",
    "eslint": "^9.35.0",
    "eslint-plugin-filenames": "^1.3.2",
    "mocha": "^11.7.2",
    "nyc": "^17.1.0",
    "pegjs": "^0.10.0",
    "should": "^13.2.3",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "aws",
    "amazon",
    "cloud",
    "dynalite",
    "jest-dynalite",
    "dynamo",
    "dynamodb",
    "fake",
    "leveldb",
    "mock",
    "serverless",
    "test"
  ]
}
